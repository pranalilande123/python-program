# Write a program to find the first non-repeating character in a string.

def nonRep(s):
    n = len(s)
    for i in range(n):
        found = False
        for j in range(n):
            if i != j and s[i] == s[j]:
                found = True
                break
        if not found:
            return s[i]
        
    return '$'
s = 'college'
print(nonRep(s))
